#!/bin/bash
#

echo -n "Sisesta oma vanus $vanus: "
read vanus
if test $vanus -gt 0 -a $vanus -lt 11
then
	echo "Oled Laps"
elif test $vanus -ge 11 -a $vanus -lt 18
then
	echo "Olen Noorkuk"
elif test $vanus -ge 18 -a $vanus -lt 63
then
	echo "oled tÃ¤iskasvanu"
elif test $vanus -ge 63
then
	echo "oled senioor"
else
	echo "vigane sisend"
fi

